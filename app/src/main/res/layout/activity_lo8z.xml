<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    tools:context="com.example.shafy.wannatofly.map">


    <ImageView
        android:id="@+id/tr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top|start"
        android:src="@drawable/logf"
        android:layout_marginTop="-180dp"
        android:layout_marginLeft="-280dp" />

    <ImageView
        android:id="@+id/moving3"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:src="@drawable/cloudc"
        android:layout_marginTop="150dp"
        android:layout_marginLeft="-280dp" />

    <ImageView
        android:id="@+id/br"
        android:layout_gravity="bottom|start"
        android:layout_width="400dp"
        android:layout_height="400dp"
        android:src="@drawable/logobb"
        android:layout_marginBottom="-100dp"
        android:layout_marginLeft="-120dp" />


    <ImageView
        android:id="@+id/moving2"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:src="@drawable/cloud"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="-200dp" />

    <ImageView
        android:id="@+id/moving"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/cloud"
        android:layout_gravity="bottom|start"
        android:layout_marginLeft="-380dp"/>

    <ImageView
        android:id="@+id/bl"
        android:layout_gravity="right"
        android:layout_width="500dp"
        android:layout_height="500dp"
        android:src="@drawable/logorr"
        android:layout_marginTop="180dp"
        android:layout_marginRight="-300dp"/>





            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >

                <EditText
                    android:id="@+id/eT"
                    android:inputType="textPassword"
                    android:layout_width="350dp"
                    android:layout_height="50dp"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="60dp"
                    android:id="@+id/puzzle"
                    android:layout_above="@id/eT"
                    android:textAlignment="center"
                    android:textSize="50sp"
                    android:text=""/>
                <Button
                    android:id="@+id/btn"
                    android:layout_marginTop="20dp"
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/shape"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@id/eT"
                    android:text="جرب كده"
                    android:textSize="30sp"
                    android:onClick="showTheSec"/>

            </RelativeLayout>


    <!-- This FrameLayout insets its children based on system windows using
         android:fitsSystemWindows. -->
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true">

        <LinearLayout
            android:id="@+id/fullscreen_content_controls"
            android:layout_width="match_parent"

            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:background="@color/black_overlay"
            android:orientation="horizontal"
            tools:ignore="UselessParent">

            <Button
                android:id="@+id/dummy_button"
                android:background="#00ffffff"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/dummy_button"
                android:textColor="#00ffffff"/>

        </LinearLayout>
    </FrameLayout>




</FrameLayout>
